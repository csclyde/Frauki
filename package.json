{
  "name": "frauki",
  "productName": "frauki",
  "version": "1.0.0",
  "description": "Fun!",
  "main": "main.js",
  "scripts": {
    "serve": "http-server",
    "build": "npm run build-win & npm run build-osx & npm run build-lin",
    "build-win": "electron-packager . --overwrite --platform=win32 --arch=x64 --icon=favicon.ico --prune=true --out=build --ignore=.gitignore --ignore=Working --ignore=Design --ignore=.vscode",
    "build-osx": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=favicon.ico --prune=true --out=build --ignore=.gitignore --ignore=Working --ignore=Design --ignore=.vscode",
    "build-lin": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=favicon.ico --prune=true --out=build --ignore=.gitignore --ignore=Working --ignore=Design --ignore=.vscode"
  },
  "keywords": [],
  "author": {
    "name": "Casey Clyde",
    "email": "caseysclyde@gmail.com"
  },
  "dependencies": {
  },
  "devDependencies": {
    "http-server": "^0.11.1",
    "electron": "12.0.0",
    "electron-packager": "^15.2.0"
  }
}
